<h1>EMDR Cycle</h1>

<div class="fuelux">
  <div class="wizard" data-initialize="wizard" id="emdrWizard">
    <div class="steps-container">
      <ul class="steps">
        <li data-step="1" class="active"><span class="badge">1</span>Overview<span class="chevron"></span></li>
        <li data-step="2" class="active"><span class="badge">2</span>Assessment<span class="chevron"></span></li>
        <li data-step="3" class="active"><span class="badge">3</span>Desensitization<span class="chevron"></span></li>
        <li data-step="4" class="active"><span class="badge">4</span>Installation<span class="chevron"></span></li>
        <li data-step="5" class="active"><span class="badge">5</span>Body Scan<span class="chevron"></span></li>
        <li data-step="6" class="active"><span class="badge">6</span>Closure<span class="chevron"></span></li>
      </ul>
    </div>

    <div class="actions">
            <button type="button" class="btn btn-default btn-prev"><span class="glyphicon glyphicon-arrow-left"></span>Prev</button>
            <button type="button" class="btn btn-default btn-next" data-last="Finish Cycle">Next<span class="glyphicon glyphicon-arrow-right"></span></button>
   </div>


        <div class="step-content">
        <!-- The first panel -->
          <div class="step-pane active" data-step="1">
          
        </div>
      </div>
  </div>
</div>

